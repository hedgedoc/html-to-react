{
  "name": "@hedgedoc/html-to-react",
  "version": "1.4.1",
  "description": "Parse HTML into React components",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "jest",
    "build": "tsc",
    "prepublishOnly": "yarn format && yarn test && yarn build",
    "lint": "eslint src --ext .ts",
    "format": "prettier -c \"{src,test}/**/*.{ts,tsx,js}\"",
    "format:fix": "prettier -w \"{src,test}/**/*.{ts,tsx,js}\""
  },
  "files": [
    "LICENSES/*",
    "package.json",
    "README.md",
    "dist/*"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/hedgedoc/html-to-react.git"
  },
  "keywords": [
    "react",
    "html",
    "htmlparser",
    "htmlparser2",
    "inner html",
    "dangerouslySetInnerHTML"
  ],
  "author": "The HedgeDoc Authors",
  "license": "AGPL-3.0",
  "devDependencies": {
    "@types/jest": "28.1.4",
    "@types/react": "18.0.14",
    "@types/react-dom": "18.0.5",
    "@typescript-eslint/eslint-plugin": "5.30.3",
    "@typescript-eslint/parser": "5.30.3",
    "eslint": "8.18.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.2.1",
    "jest": "28.1.2",
    "prettier": "2.7.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "ts-jest": "28.0.5",
    "typescript": "4.7.4"
  },
  "dependencies": {
    "domhandler": "^5.0.3",
    "htmlparser2": "^8.0.0"
  },
  "peerDependencies": {
    "react": ">=16.0"
  },
  "directories": {
    "test": "test"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx)$",
    "testPathIgnorePatterns": [
      "/dist/"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ]
  },
  "packageManager": "yarn@3.2.1"
}
